{
  "cli": {
    "version": ">= 0.60.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview_apk_dev": {
      "developmentClient": true,
      "android": {
        "buildType": "apk",
        "distribution": "internal"
      },
      "env": {
        "ANDROID_SDK_ROOT": "/Users/yfd/Library/Android/sdk",
       "EX_UPDATES_ANDROID_DELAY_LOAD_APP": "false"
      },
      "channel": "preview_apk_dev"
    },
    "preview_ios_dev": {
      "distribution": "internal",
      "developmentClient": true,
      "ios": {
        "simulator": false,
        "distribution": "internal",
        "resourceClass": "m-medium"
      },
      "channel": "preview_ios_dev"
    },
    "preview_ios_simulator": {
      "developmentClient": true,
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      },
      "channel": "preview_ios_simulator"
    },
    "preview_apk": {
      "android": {
        "buildType": "apk",
        "distribution": "internal"
      },
      "env": {
        "ANDROID_SDK_ROOT": "/Users/yfd/Library/Android/sdk"
      },
      "channel": "preview_apk"
    },
    "preview_aab": {
      "env": {
        "ANDROID_SDK_ROOT": "/Users/yfd/Library/Android/sdk",
        "EX_UPDATES_ANDROID_DELAY_LOAD_APP": "false"
      },
      "channel": "preview_aab"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "ANDROID_SDK_ROOT": "/Users/yfd/Library/Android/sdk"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {},
    "preview_ios_dev": {}
  }
}